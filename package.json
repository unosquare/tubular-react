{
  "name": "tubular-react",
  "version": "0.2.6",
  "description": "Tubular React",
  "main": "dist/index.js",
  "scripts": {
    "build:docs": "webpack --env.NODE_ENV=docs --mode=production --progress",
    "build": "tsc --p ./tsconfigs/tsconfig.tubular.json",
    "sample": "webpack --env.NODE_ENV=sample --mode=development --progress",
    "start": "webpack-dev-server --env.NODE_ENV=sample --open --progress",
    "lint": "tslint src/**/*.ts src/**/*.tsx",
    "test": "jest",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "test:coveralls": "nyc npm test && nyc report --reporter=text-lcov",
    "prepublishOnly": "npm run build"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/setupTest.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/test/**/*.spec.(ts|tsx)"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.base.json"
      }
    }
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "next.config.js",
      "test/**/*.*",
      "dist/**/*.*",
      "sample/**/*.*",
      "docs/**/*.*",
      "pages/**/*.*",
      "srcdocs/**/*.*",
      "src/Util/index.js",
      "webpack.config.js"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unosquare/tubular-react.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/unosquare/tubular-react/issues"
  },
  "homepage": "https://github.com/unosquare/tubular-react#readme",
  "dependencies": {
    "@material-ui/core": "^1.2.0",
    "@material-ui/icons": "^1.1.0",
    "@types/vinyl": "^2.0.2",
    "axios": "^0.18.0",
    "jest": "^23.0.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-test-renderer": "^16.4.0",
    "rxjs-es": "^5.0.0-beta.12",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/lodash": "^4.14.109",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-router": "^4.0.25",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-syntax-highlighter": "0.0.5",
    "@types/rx": "^4.1.1",
    "@types/sinon": "^5.0.0",
    "@zeit/next-typescript": "1.0.1",
    "awesome-typescript-loader": "^5.0.0",
    "axios-mock-adapter": "^1.15.0",
    "browser-sync": "^2.24.4",
    "browserify": "^16.2.2",
    "coveralls": "^3.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jsdom": "^11.11.0",
    "nyc": "^11.8.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-syntax-highlighter": "^7.0.4",
    "router5": "^6.2.2",
    "sinon": "^5.0.10",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.3.0",
    "ts-node": "^6.0.5",
    "tsify": "^4.0.0",
    "tslint": "^5.10.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.8.3",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.0.1",
    "webpack-dev-server": "^3.1.4"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  }
}
