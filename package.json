{
  "name": "tubular-react",
  "version": "0.1.6",
  "description": "Tubular React",
  "main": "./src/index",
  "scripts": {
    "start": "gulp watch",
    "build": "gulp",
    "lint": "tslint src/**/*.ts src/**/*.tsx",
    "test": "gulp test",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "test:coveralls": "nyc npm test && nyc report --reporter=text-lcov",
    "docs:dev": "next",
    "docs:start": "next start",
    "docs": "set NODE_ENV=production && gulp && next build && next export -o docs"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "gulpfile.js",
      "next.config.js",
      "test/**/*.*",
      "build/**/*.*",
      "sample/**/*.*",
      "docs/**/*.*",
      "pages/**/*.*",
      "srcdocs/**/*.*",
      "src/Util/index.js"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unosquare/tubular-react.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/unosquare/tubular-react/issues"
  },
  "homepage": "https://github.com/unosquare/tubular-react#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "lodash": "^4.17.5",
    "material-ui": "next",
    "material-ui-icons": "^1.0.0-beta.17",
    "material-ui-pickers": "^1.0.0-beta.12",
    "moment": "^2.20.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rxjs-es": "^5.0.0-beta.12",
    "typeface-roboto": "0.0.50"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/lodash": "^4.14.102",
    "@types/mocha": "^2.2.48",
    "@types/next": "^2.4.8",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "@types/rx": "^4.1.1",
    "@zeit/next-typescript": "0.0.10",
    "awesome-typescript-loader": "^3.4.1",
    "axios-mock-adapter": "^1.12.0",
    "browser-sync": "^2.23.5",
    "browserify": "^15.2.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "del": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "gulp": "^3.9.1",
    "gulp-copy": "^1.1.0",
    "gulp-mocha": "^5.0.0",
    "gulp-typescript": "^3.2.4",
    "jsdom": "^11.5.1",
    "mock-local-storage": "^1.0.5",
    "next": "^5.0.0",
    "nyc": "^11.4.1",
    "react-syntax-highlighter": "^7.0.0",
    "sinon": "^4.1.6",
    "ts-loader": "^3.5.0",
    "ts-node": "^4.1.0",
    "tsify": "^3.0.4",
    "tslint": "^5.9.1",
    "tslint-react": "^3.4.0",
    "typescript": "^2.7.2",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.9.0",
    "webpack": "^3.11.0"
  },
  "peerDependencies": {
    "material-ui": "next",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  }
}
