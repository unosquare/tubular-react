{
  "name": "tubular-react",
  "version": "0.2.0",
  "description": "Tubular React",
  "main": "./src/index",
  "scripts": {
    "start": "gulp watch",
    "build": "gulp",
    "lint": "tslint src/**/*.ts src/**/*.tsx",
    "test": "jest",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "test:coveralls": "nyc npm test && nyc report --reporter=text-lcov",
    "docs:run": "webpack -d",
    "docs": "webpack -p"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/setupTest.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/test/**/*.spec.(ts|tsx)"
    ]
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "gulpfile.js",
      "next.config.js",
      "test/**/*.*",
      "build/**/*.*",
      "sample/**/*.*",
      "docs/**/*.*",
      "pages/**/*.*",
      "srcdocs/**/*.*",
      "src/Util/index.js",
      "webpack.config.js"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unosquare/tubular-react.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/unosquare/tubular-react/issues"
  },
  "homepage": "https://github.com/unosquare/tubular-react#readme",
  "dependencies": {
    "@material-ui/core": "^1.0.0",
    "@material-ui/icons": "^1.0.0",
    "axios": "^0.18.0",
    "babel-jest": "^23.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "jest": "^23.0.1",
    "lodash": "^4.17.10",
    "material-ui-pickers": "^1.0.0-rc.9",
    "moment": "^2.22.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-test-renderer": "^16.4.0",
    "rxjs-es": "^5.0.0-beta.12",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/lodash": "^4.14.109",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-router": "^4.0.25",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-syntax-highlighter": "0.0.5",
    "@types/rx": "^4.1.1",
    "@types/sinon": "^5.0.0",
    "@zeit/next-typescript": "1.0.1",
    "awesome-typescript-loader": "^5.0.0",
    "axios-mock-adapter": "^1.15.0",
    "browser-sync": "^2.24.4",
    "browserify": "^16.2.2",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "del": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "gulp": "^3.9.1",
    "gulp-copy": "^1.1.0",
    "gulp-typescript": "^4.0.2",
    "jsdom": "^11.11.0",
    "nyc": "^11.8.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-syntax-highlighter": "^7.0.4",
    "router5": "^6.2.2",
    "sinon": "^5.0.10",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.3.0",
    "ts-node": "^6.0.5",
    "tsify": "^4.0.0",
    "tslint": "^5.10.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.8.3",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.0",
    "webpack": "^4.8.3"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  }
}
